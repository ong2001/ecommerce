SELECT * FROM 'products' WHERE item_status='A*,
SELECT * FROM 'products' WHERE item_status='I*;
SELECT o.order_id, o.date_ordered, u.username, p.item_name, SUM(item_qty * item_price) as total_amount
FROM orders o
JOIN users u ON o.user_id = u.user_id
JOIN products p ON o.item_id = p.item_id
GROUP BY p.order_id;
SELECT o.order_status, o.order_id, u.sername, p.item_name, SUM(item_qty
* item_price) as total_amount,
SUM(item_qty) as total_qty
FROM orders o
JOIN users u ON o.user_id = u.user_id
JOIN products p ON o.item id = p.item id
WHERE o.order_status='D'
GROUP BY o.order_status;
SELECT p.item_id, p.item_name, o.item_qty, u.username, u.user_status, o.date_ordered, o.order_status
FROM orders o
JOIN users u
   ON o.user_id = u.user_id
JOIN products p
   ON o.item_id = p.item_id
WHERE u.user_status = 'A'
AND p.item_status = 'A'
AND o.order_status = 'P'
AND o.date_ordered >= '2023-01-01'
GROUP BY o.order_id;
